{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "pragma": "Framework.createElement",
        "pragmaFrag": "Framework.Fragment",
        "throwIfNamespace": false,
        "runtime": "classic"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "Framework.createElement",
        "pragmaFrag": "Framework.Fragment"
      }
    ]
  ]
}
